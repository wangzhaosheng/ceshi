            />


        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click56"
            android:text="截取字符串"

            />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click57"
