        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click38"
            android:text="获取通知使用权" />


        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click39"
