            />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click50"
            android:text="空字符串拼接字符串问题"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click51"
            android:text="截取address"

            />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click52"
