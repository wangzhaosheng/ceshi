        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click45"
            android:text="空格字符检测" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="click46"
